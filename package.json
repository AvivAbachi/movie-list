{
	"name": "movie-list",
	"version": "1.0.0",
	"description": "Movie list build whit React and use whit 3 different state managers(Hooks, Mobx, Redux)",
	"dependencies": {
		"@fullhuman/postcss-purgecss": "^4.0.3",
		"@types/node": "^12.19.3",
		"@types/react": "^16.9.55",
		"@types/react-dom": "^16.9.9",
		"@types/react-redux": "^7.1.9",
		"@types/react-router-dom": "^5.1.6",
		"autoprefixer": "^10.2.5",
		"cross-env": "^7.0.2",
		"cssnano": "^5.0.2",
		"framer-motion": "^2.9.4",
		"mobx": "^6.0.1",
		"mobx-react-lite": "^3.0.1",
		"postcss": "^8.2.15",
		"postcss-cli": "^8.2.0",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-redux": "^7.2.2",
		"react-router-dom": "^5.2.0",
		"react-scripts": "^2.1.8",
		"redux": "^4.0.5",
		"tailwindcss": "^2.1.2",
		"typescript": "^4.0.5",
		"watch": "^0.13.0"
	},
	"scripts": {
		"start": "react-scripts start",
		"watch:css": "watch --wait=1 --interval=1 \"npm run build:css\" src/assets/",
		"build": "cross-env NODE_ENV=production npm run build:css && react-scripts build",
		"build:css": "postcss src/assets/tailwind.css -o src/styles.css",
		"serve": "serve -d build",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
