		"typescript": "^4.0.5",
		"watch": "^1.0.2",
		"web-vitals": "^0.2.4"
	},
	"scripts": {
		"start": "react-scripts start",
		"watch:css": "watch --wait=1 --interval=1 \"npm run build:css\" src/assets/",
		"build": "cross-env NODE_ENV=production npm run build:css && react-scripts build",
		"build:css": "postcss src/assets/tailwind.css -o src/styles.css",
		"serve": "serve -d build",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
