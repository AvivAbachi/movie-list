		"@testing-library/react": "^11.1.0",
		"@testing-library/user-event": "^12.1.10",
		"@types/jest": "^26.0.15",
		"@types/node": "^12.19.3",
		"@types/react": "^16.9.55",
		"@types/react-dom": "^16.9.9",
		"@types/react-redux": "^7.1.9",
		"@types/react-router-dom": "^5.1.6",
		"autoprefixer": "^9.8.6",
		"cross-env": "^7.0.2",
		"cssnano": "^4.1.10",
		"framer-motion": "^2.9.4",
		"mobx": "^6.0.1",
		"mobx-react-lite": "^3.0.1",
		"postcss": "^7.0.35",
		"postcss-cli": "^8.2.0",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-redux": "^7.2.2",
		"react-router-dom": "^5.2.0",
